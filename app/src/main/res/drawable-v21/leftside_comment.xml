<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/bottom_divider"
        android:left="-20dp"
        android:right="-20dp"
        android:top="-20dp">
        <shape android:shape="rectangle">
            <solid />
            <stroke
                android:width="1dp"
                android:color="?colorDivider"/>
        </shape>
    </item>
    <item
        android:id="@+id/leftside_shapeitem"
        android:bottom="-20dp"
        android:right="-20dp"
        android:top="-20dp">
        <shape android:shape="rectangle">
            <solid android:color="#00000000"/>
        </shape>
    </item>
</layer-list>